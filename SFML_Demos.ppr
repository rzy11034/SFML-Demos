program SFML_Demos;

{$mode objfpc}{$H+}
{$ModeSwitch unicodestrings}{$J-}

uses
  {$IFDEF UNIX}
  cthreads,
  {$ENDIF}
  Classes,
  SysUtils,
  gles20,
  SFML_Demos.Main,
  SFML_Demos.HelloWorld,
  SFML_Demos.Basic,
  SFML_Demos.Caleidos, SFML_Demos.Circle, SFML_Demos.Circles, SFML_Demos.Colorful;

begin
  try
    Run;
  except
    on E: Exception do
      WriteLn(E.ClassName, ': ', E.Message);
  end;
end.
